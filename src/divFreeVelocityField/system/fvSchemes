FoamFile {
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}

divSchemes {
    default         Gauss linear;
}

gradSchemes {
    default         Gauss linear;
}

laplacianSchemes {
    default         Gauss linear corrected;
}

ddtSchemes {
    default         backward;
}

fluxRequired {
    default         no;
    p;
}
