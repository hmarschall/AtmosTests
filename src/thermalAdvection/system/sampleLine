FoamFile {
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sampleDict;
}

setFormat raw;

interpolationScheme cell;

fields (
    T_diff
);

sets (
    line {
        type        midPoint;
        axis        z;

        start       (9e3 0.5e3 0);
        end         (9e3 0.5e3 7.5e3);
    }

);

